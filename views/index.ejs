<% if (!user) { %>
    <h2>Welcome! Please log in.</h2>
    <a href="/login">Log In</a>
    <% } else { %>
        <h2>Hello,
            <%= user.familyName +" " + user.givenName %>.</h2>
        <a href="/account">Account Info</a>
        </br>
        <a href="/logout">Log Out</a>
        <% } %>